{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": 2,
        "semi": "error",
        "no-console": "off",
        "quotes": ["error", "double"],
        "object-curly-spacing": ["error", "always"]
    }
}

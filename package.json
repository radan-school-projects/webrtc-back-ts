    "@typescript-eslint/parser": "^4.14.1",
    "babel-loader": "^8.2.2",
    "dotenv-webpack": "^7.0.1",
    "eslint": "^7.18.0",
    "eslint-config-google": "^0.14.0",
    "nodemon-webpack-plugin": "^4.4.4",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.17.0",
